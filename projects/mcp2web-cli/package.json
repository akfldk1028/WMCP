{
  "name": "mcp2web",
  "version": "0.1.0",
  "description": "Convert MCP server tools to WebMCP browser code - CLI & library",
  "type": "module",
  "bin": {
    "mcp2web": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "webmcp",
    "mcp",
    "ai-agent",
    "chrome",
    "browser",
    "converter"
  ],
  "license": "MIT",
  "dependencies": {
    "@wmcp/core": "workspace:*",
    "@wmcp/mcp-bridge": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.4.0"
  }
}
